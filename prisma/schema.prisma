generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bar {
  id            String   @id @default(cuid())
  nombre        String
  direccion     String?
  latitud       Float
  longitud      Float
  notaTortilla  Int
  comentario    String?
  nombreUsuario String
  createdAt     DateTime @default(now())
}
